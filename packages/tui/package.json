    "enquirer": "2.3.6",
    "stdout-update": "1.6.8"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.3",
    "jest": "29.5.0",
    "typescript": "5.0.2"
  }
