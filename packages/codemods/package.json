{
  "name": "@redwoodjs/codemods",
  "version": "0.36.4",
  "license": "MIT",
  "bin": "./dist/codemods.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@redwoodjs/internal": "0.36.4",
    "execa": "5.1.1",
    "fast-glob": "^3.2.7",
    "jscodeshift": "0.13.0",
    "tempy": "1.0.1"
  },
  "scripts": {
    "build": "yarn build:js",
    "build:js": "babel src -d dist --extensions \".js,.ts\" --copy-files --no-copy-ignored && yarn build:clean-dist",
    "build:clean-dist": "rimraf 'dist/**/*/{__tests__,__testfixtures__}'",
    "build:watch": "nodemon --watch src --ignore dist,template --exec \"yarn build\"",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/jscodeshift": "^0.11.2"
  }
}
