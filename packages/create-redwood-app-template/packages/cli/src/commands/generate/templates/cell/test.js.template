import { render, cleanup } from '@testing-library/react'

import { Loading, Empty, Failure, Success } from './${camelName}Cell'

const error = { message: 'error message' }

describe('${camelName}Cell', () => {
  afterEach(() => {
    cleanup()
  })
  it('Loading renders successfully', () => {
    expect(() => {
      render(<Loading />)
    }).not.toThrow()
  })
  it('Empty renders successfully', () => {
    expect(() => {
      render(<Empty />)
    }).not.toThrow()
  })
  it('Failure renders successfully', () => {
    expect(() => {
      render(<Failure error={error} />)
    }).not.toThrow()
  })
  it('Success renders successfully', () => {
    expect(() => {
      Success('${camelName}Cell')
    }).not.toThrow()
  })
})
