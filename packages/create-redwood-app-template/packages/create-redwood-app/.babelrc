{
  "extends": "../../babel.config.js",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "12.13.0"
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "src": "./src"
        }
      }
    ]
  ]
}
