    "dev": "nodemon --exec babel-node src/index.js",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@hammerframework/hammer-core": "^0.0.1-alpha.8",
    "axios": "^0.19.0",
    "camelcase": "^5.3.1",
    "decompress": "^4.2.0",
    "ink": "^2.2.0",
    "react": "^16.8.6",
    "require-dir": "^1.2.0",
    "tmp": "^0.1.0",
    "yargs-parser": "^13.1.1",
    "yarn-or-npm": "^3.0.1"
  },
  "devDependencies": {
    "ink-testing-library": "^1.0.2"
  }
}
