export type GetEntry = (rscId: string) => Promise<
  | React.FunctionComponent
  | {
      default: React.FunctionComponent
    }
  | null
>

export function defineEntries(getEntry: GetEntry) {
  return {
    getEntry,
  }
}

export default defineEntries(
  // getEntry
  async (id) => {
    switch (id) {
      case 'App':
        return import('./App')
      default:
        return null
    }
  }
)
