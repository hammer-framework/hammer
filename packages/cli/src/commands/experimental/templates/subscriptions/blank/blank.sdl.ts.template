export const schema = gql`
  type ${typeName} {
    from: String
    body: String
  }

  type Query {
    ${queryName}(id: ID!): [${typeName}!]! @skipAuth
  }

  input Create${typeName}Input {
    id: ID!
    from: String!
    body: String!
  }

  type Mutation {
    create${typeName}(input: Create${typeName}Input!): ${typeName}! @skipAuth
  }
`
