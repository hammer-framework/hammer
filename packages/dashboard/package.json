{
  "name": "@redwoodjs/dashboard",
  "version": "0.0.1",
  "description": "Redwood's development dashboard",
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/dashboard"
  },
  "license": "MIT",
  "main": "dist-backend/index",
  "files": [
    "dist-backend",
    "dist-frontend"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:types",
    "build:js": "babel backend -d dist-backend --extensions \".js,.ts,.tsx\"",
    "build:types": "tsc --build --verbose",
    "build:watch": "nodemon --watch src --ext \"js,ts,tsx\" --ignore dist --exec \"yarn build && yarn fix:permissions\"",
    "fix:permissions": "chmod +x dist/index.js; chmod +x dist/watch.js",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "jest src",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/runtime-corejs3": "7.20.13",
    "@fastify/http-proxy": "8.4.0",
    "@fastify/static": "6.9.0",
    "@fastify/url-data": "5.3.1",
    "@redwoodjs/internal": "4.0.0",
    "ansi-colors": "4.1.3",
    "chalk": "4.1.2",
    "chokidar": "3.5.3",
    "core-js": "3.27.2",
    "crypto-js": "4.1.1",
    "dotenv": "^16.0.3",
    "fast-json-parse": "1.0.3",
    "fastify": "4.13.0",
    "fastify-raw-body": "4.2.0",
    "graphql": "16.6.0",
    "graphql-yoga": "3.7.0",
    "jsonwebtoken": "9.0.0",
    "lodash.escape": "4.0.1",
    "pretty-bytes": "5.6.0",
    "qs": "6.11.0",
    "split2": "4.1.0",
    "sqlite": "4.1.2",
    "sqlite3": "5.1.4",
    "uuid": "9.0.0",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@types/aws-lambda": "8.10.110",
    "@types/crypto-js": "4.1.1",
    "@types/jsonwebtoken": "9.0.1",
    "@types/lodash.escape": "4.0.7",
    "@types/qs": "6.9.7",
    "@types/split2": "3.2.1",
    "@types/uuid": "9.0.1",
    "@types/yargs": "17.0.22",
    "aws-lambda": "1.0.7",
    "jest": "29.4.2",
    "typescript": "4.9.5"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
